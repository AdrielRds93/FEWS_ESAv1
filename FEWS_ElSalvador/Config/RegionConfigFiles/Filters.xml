<?xml version="1.0" encoding="UTF-8"?>
<filters version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/filters.xsd">
	<filter id="Estaciones">
		<child foreignKey="Convencionales"/>
		<child foreignKey="Telemétricas"/>
	</filter>
	<filter id="Cuencas">
		<child foreignKey="Observaciones"/>
		<child foreignKey="Pronóstico"/>
	</filter>
	<filter id="Simulaciones">
		<child foreignKey="LempaAlta_HBV"/>
	</filter>
	<filter id="LempaAlta_HBV" name="HBV Lempa Alta">
		<child foreignKey="LempaAlta_HBV_sinDA"/>
		<child foreignKey="LempaAlta_HBV_conDA"/>
	</filter>
	<filter id="Observaciones">
		<timeSeriesSet>
			<moduleInstanceId>PreprocessObs</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.obs</parameterId>
			<locationSetId>SubbasinLempa</locationSetId>
			<timeSeriesType>simulated historical</timeSeriesType>
			<timeStep unit="hour"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>PreprocessT</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>T.obs</parameterId>
			<locationSetId>SubbasinLempa</locationSetId>
			<timeSeriesType>simulated historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</filter>
	<filter id="Pronóstico">
		<timeSeriesSet>
			<moduleInstanceId>PreprocessWRF</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.pro</parameterId>
			<qualifierId>WRF</qualifierId>
			<locationSetId>SubbasinLempa</locationSetId>
			<timeSeriesType>simulated forecasting</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="day" start="0" end="5"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</filter>
	<filter id="Convencionales">
		<timeSeriesSet>
			<moduleInstanceId>ImportMARN</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.obs</parameterId>
			<locationSetId>MARN_CONVENCIONAL</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep id="daily07"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportMARN</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>Tmax.obs</parameterId>
			<locationSetId>MARN_T_diaria</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep id="daily07"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>ImportMARN</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>Tmin.obs</parameterId>
			<locationSetId>MARN_T_diaria</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep id="daily07"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</filter>
	<filter id="Telemétricas">
		<timeSeriesSet>
			<moduleInstanceId>PreprocessP</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.obs</parameterId>
			<locationSetId>MARN_P</locationSetId>
			<timeSeriesType>simulated historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>PreprocessT</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>T.obs</parameterId>
			<locationSetId>MARN_T</locationSetId>
			<timeSeriesType>simulated historical</timeSeriesType>
			<timeStep unit="hour" multiplier="1"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</filter>
