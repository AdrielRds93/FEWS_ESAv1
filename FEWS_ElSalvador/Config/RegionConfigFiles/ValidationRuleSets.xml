		</timeSeriesSet>
	</validationRuleSet>
	<validationRuleSet validationRuleSetId="Precipitacion_24h" timeZone="0">
		<extremeValues>
			<hardMax constantLimit="500"/>
			<hardMin constantLimit="0.0"/>
		</extremeValues>
		<timeSeriesSet>
			<moduleInstanceId>ImportMARN</moduleInstanceId>
			<valueType>scalar</valueType>
