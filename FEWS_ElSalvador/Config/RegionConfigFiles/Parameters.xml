<?xml version="1.0" encoding="UTF-8"?>
<parameterGroups version="1.0" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/parameters.xsd">
	<parameterGroup id="Precipitation" name="Precipitación">
		<parameterType>accumulative</parameterType>
		<unit>mm</unit>
		<valueResolution>0.01</valueResolution>
		<parameter id="P.obs" name="P.obs">
			<shortName>P.obs</shortName>
		</parameter>
		<parameter id="P.obs.acc" name="P.obs.acc">
			<shortName>P.obs.acc</shortName>
		</parameter>
		<parameter id="P.pro" name="P.pro">
			<shortName>P.pro</shortName>
		</parameter>
		<parameter id="P.pro.acc" name="P.pro.acc">
			<shortName>P.pro.acc</shortName>
		</parameter>
	</parameterGroup>
	<parameterGroup id="Evapotranspiración potencial">
		<parameterType>instantaneous</parameterType>
		<unit>mm</unit>
		<valueResolution>0.1</valueResolution>
		<parameter id="E.obs" name="E.obs">
			<shortName>E</shortName>
		</parameter>
		<parameter id="E.sim" name="E.sim">
			<shortName>E.sim</shortName>
		</parameter>
		<parameter id="E.pro" name="E.pro">
			<shortName>E.pro</shortName>
		</parameter>
	</parameterGroup>
	<parameterGroup id="Temperatura">
		<parameterType>instantaneous</parameterType>
		<unit>oC</unit>
		<valueResolution>0.1</valueResolution>
		<parameter id="T.obs" name="T.obs">
			<shortName>T.obs</shortName>
		</parameter>
		<parameter id="Tmed.obs" name="Tmed.obs">
			<shortName>Tmed.obs</shortName>
		</parameter>
		<parameter id="Tmin.obs" name="Tmin.obs">
			<shortName>Tmin.obs</shortName>
		</parameter>
		<parameter id="Tmax.obs" name="Tmax.obs">
			<shortName>Tmax.obs</shortName>
		</parameter>
		<parameter id="T.pro" name="T.pro">
			<shortName>T.pro</shortName>
		</parameter>
	</parameterGroup>
	<parameterGroup id="Caudal">
		<parameterType>mean</parameterType>
		<unit>m3/s</unit>
		<valueResolution>0.01</valueResolution>
		<parameter id="Q.obs" name="Q.obs">
			<shortName>Q.obs</shortName>
		</parameter>
		<parameter id="Q.sim" name="Q.sim">
			<shortName>Q.sim</shortName>
		</parameter>
		<parameter id="Q.pro" name="Q.pro">
			<shortName>Q.pro</shortName>
		</parameter>
	</parameterGroup>
	<parameterGroup id="Nivel de agua">
		<parameterType>instantaneous</parameterType>
		<unit>m</unit>
		<valueResolution>0.01</valueResolution>
		<usesDatum>true</usesDatum>
		<parameter id="H.obs" name="H.obs">
			<shortName>H.obs</shortName>
		</parameter>
		<parameter id="H.sim" name="H.sim">
			<shortName>H.sim</shortName>
		</parameter>
		<parameter id="H.pro" name="H.pro">
			<shortName>H.pro</shortName>
		</parameter>
	</parameterGroup>
	<parameterGroup id="Factor">
		<parameterType>instantaneous</parameterType>
		<unit>-</unit>
		<valueResolution>0.01</valueResolution>
		<parameter id="Factor" name="Factor">
			<shortName>Factor</shortName>
		</parameter>
	</parameterGroup>
	<parameterGroup id="Otros">
		<parameterType>instantaneous</parameterType>
		<unit>-</unit>
		<valueResolution>0.001</valueResolution>
		<parameter id="Fraction" name="Fraction">
			<shortName>Fraction</shortName>
			<valueResolution>0.0001</valueResolution>
		</parameter>
	</parameterGroup>
</parameterGroups>
