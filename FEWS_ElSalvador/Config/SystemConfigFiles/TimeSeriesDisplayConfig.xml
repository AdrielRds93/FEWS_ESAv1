			<movingAccumulationTimeSpan unit="week" multiplier="1"/>
			<movingAccumulationTimeSpan unit="day" multiplier="30"/>
		</statisticalFunction>
		<statisticalFunction function="accumulationAggregation" ignoreMissings="true">
			<movingAccumulationTimeSpan unit="hour" multiplier="1"/>
			<movingAccumulationTimeSpan unit="hour" multiplier="4"/>
			<movingAccumulationTimeSpan unit="hour" multiplier="8"/>
			<movingAccumulationTimeSpan unit="hour" multiplier="12"/>
			<movingAccumulationTimeSpan unit="day" multiplier="1"/>
			<movingAccumulationTimeSpan unit="day" multiplier="2"/>
