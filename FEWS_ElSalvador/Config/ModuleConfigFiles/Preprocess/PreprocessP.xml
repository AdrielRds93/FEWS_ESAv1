					<variableId>MARN_10min</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_10minToDay</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="MARN_Aggregation_15minToDaily">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>MARN_15min</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_15minToDay</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="Merge daily data">
		<merge>
			<simple>
				<inputVariable>
					<variableId>MARN_day</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>MARN_5minToDay</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>MARN_10minToDay</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>MARN_15minToDay</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>dayMerged</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<!-- Spatial interpolation (Thiessen) in subbasin Lempa-->
	<transformation id="ThiessenPolygon day">
		<interpolationSpatial>
			<thiessenPolygon>
				<inputVariable>
					<variableId>dayMerged</variableId>
				</inputVariable>
				<distanceGeoDatum>UTM21S</distanceGeoDatum>
				<outputVariable>
					<variableId>dayCuencas</variableId>
				</outputVariable>
			</thiessenPolygon>
		</interpolationSpatial>
	</transformation>
	<transformation id="ThiessenPolygon hour">
		<interpolationSpatial>
			<thiessenPolygon>
				<inputVariable>
					<variableId>hourMerged</variableId>
				</inputVariable>
				<distanceGeoDatum>UTM21S</distanceGeoDatum>
				<outputVariable>
					<variableId>hourCuencas</variableId>
				</outputVariable>
			</thiessenPolygon>
		</interpolationSpatial>
	</transformation>
	<!-- Combine hourly and daily -->
	<transformation id="cuencaDay2hour">
		<disaggregation>
			<accumulative>
				<inputVariable>
					<variableId>dayCuencas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>dayCuencas.hour</variableId>
				</outputVariable>
			</accumulative>
		</disaggregation>
	</transformation>
	<transformation id="cuencaHour2day">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>hourCuencas</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>hourCuencas.day</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="CalcFractions">
		<user>
			<simple>
				<expression>dayCuencas/hourCuencas.day</expression>
				<outputVariable>
					<variableId>fraction.day</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<transformation id="CalcFractions">
		<disaggregation>
			<meanToMean>
				<inputVariable>
					<variableId>fraction.day</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>fraction.hour</variableId>
				</outputVariable>
			</meanToMean>
		</disaggregation>
	</transformation>
	<transformation id="expression">
		<user>
			<simple>
				<expression>fraction.hour*hourCuencas</expression>
				<outputVariable>
					<variableId>hourCuencas.hour</variableId>
				</outputVariable>
			</simple>
		</user>
	</transformation>
	<transformation id="MergeCompleteWithHour">
		<merge>
			<simple>
				<inputVariable>
					<variableId>hourCuencas.hour</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>hourCuencas</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>dayCuencas.hour</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Cuenca.hour</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<transformation id="Aggregate">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>Cuenca.hour</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>Cuenca.3hour</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
</transformationModule>
