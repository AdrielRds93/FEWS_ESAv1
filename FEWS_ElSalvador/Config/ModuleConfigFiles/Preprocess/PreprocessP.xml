	<!--Deaccumulate rainfall to inst rainfall-->
	<transformation id="MARN_Instantaneous_5min">
		<deaccumulation>
			<sum>
				<inputVariable>
					<variableId>MARN_5min_acc</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_5min</variableId>
				</outputVariable>
			</sum>
		</deaccumulation>
	</transformation>
	<transformation id="MARN_Instantaneous_10min">
		<deaccumulation>
			<sum>
				<inputVariable>
					<variableId>MARN_10min_acc</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_10min</variableId>
				</outputVariable>
			</sum>
		</deaccumulation>
	</transformation>
	<transformation id="MARN_Instantaneous_15min">
		<deaccumulation>
			<sum>
				<inputVariable>
					<variableId>MARN_15min_acc</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_15min</variableId>
				</outputVariable>
			</sum>
		</deaccumulation>
	</transformation>
	<!--Aggregate to HOURLY measurement and combine it-->
	<transformation id="MARN_Aggregation_5minToHourly">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>MARN_5min</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_5minToHourly</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="MARN_Aggregation_10minToHourly">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>MARN_10min</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_10minToHourly</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="MARN_Aggregation_15minToHourly">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>MARN_15min</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_15minToHourly</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="Merge hourly data">
		<merge>
			<simple>
				<inputVariable>
					<variableId>MARN_5minToHourly</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>MARN_10minToHourly</variableId>
				</inputVariable>
				<inputVariable>
					<variableId>MARN_15minToHourly</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>hourMerged</variableId>
				</outputVariable>
			</simple>
		</merge>
	</transformation>
	<!--Aggregate to DAILY measurement and combine it-->
	<transformation id="MARN_Aggregation_5minToDaily">
		<aggregation>
			<accumulative>
				<inputVariable>
					<variableId>MARN_5min</variableId>
				</inputVariable>
				<outputVariable>
					<variableId>MARN_5minToDay</variableId>
				</outputVariable>
			</accumulative>
		</aggregation>
	</transformation>
	<transformation id="MARN_Aggregation_10minToDaily">
