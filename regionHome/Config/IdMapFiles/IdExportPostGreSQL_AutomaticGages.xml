<?xml version="1.0" encoding="UTF-8"?>
<idMap
    xmlns="http://www.wldelft.nl/fews"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.wldelft.nl/fews http://schemas/idMap.xsd"
    version="1.1"
>
    <!--Hourly incremental precipitation-->
    <function internalLocationSet="1HrPrecipGages" internalParameter="PP" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="PH"/>
    <!--15-min incremental precipitation-->
    <function internalLocationSet="15MinPrecipGages" internalParameter="PP" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="P4"/>
    <!--10-min incremental precipitation-->
    <function internalLocationSet="10MinPrecipGages" internalParameter="PP" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="P6"/>
    <!--Accumulated precipitation-->
    <function internalLocationSet="AllGages" internalParameter="PC" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="PC"/>
    <!--Stage-->
    <function internalLocationSet="AllGages" internalParameter="STG" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="HG"/>
    <!--Pool elevation-->
    <function internalLocationSet="AllGages" internalParameter="PELV" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="HP"/>
    <!--Reservoir inflow (CEL-estimated)-->
    <function internalLocationSet="AllGages" internalParameter="RQIN" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="QI"/>
    <!--Reservoir outflow, spillway-->
    <function internalLocationSet="AllGages" internalParameter="RQSW" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="QS"/>
    <!--Reservoir outflow, turbine-->
    <function internalLocationSet="AllGages" internalParameter="DQIN" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="QG"/>
    <!--Reservoir outflow, total-->
    <function internalLocationSet="AllGages" internalParameter="RQOT" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="QT"/>
    <enableOneToOneMapping/>
    
</idMap>
