<?xml version="1.0" encoding="UTF-8"?>
<manualForecastDisplay xmlns="http://www.wldelft.nl/fews"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.wldelft.nl/fews http://schemas/manualForecastDisplay.xsd">

	<coldState>
		<startDate unit="hour" multiplier="72"/>
	</coldState>
	<warmState>
		<stateSearchPeriod unit="hour" start="-240" end="-24"/>
	</warmState>
	<forecastLength unit="hour" multiplier="0"/>

</manualForecastDisplay>
