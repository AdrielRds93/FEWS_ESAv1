<!-- QA/QC workflows ===========================================================-->
    <workflowDescriptor id="ImportPostGreSQL_AutomaticGages_AccumulatedOnly" name="Import Automatic Accumulated Precipitation (do QC)" forecast="false" visible="true" allowApprove="false">
        <description>Import the scalar automatic accumulated precipitation time series from the Hidrologia PostGreSQL database</description>
        <runExpiryTime unit="day" multiplier="120"/>
    </workflowDescriptor>
    <workflowDescriptor id="ExportPostGreSQL_AutomaticAccumulated" name="Export QC'ed Accumulated Precipitation to DB" forecast="false" visible="true" allowApprove="false">
        <description>Write the quality controlled accumulated precipitation time series to the Hidrologia PostGreSQL database</description>
    </workflowDescriptor>
    <workflowDescriptor id="ImportPostGreSQL_AutomaticGages" name="Import All Automatic Data (do QC)" forecast="false" visible="true" allowApprove="false">
        <description>Import all of the scalar automatic time series (accumulated and incremental precipitation, stage, and reservoir time series from the Hidrologia PostGreSQL database</description>
        <runExpiryTime unit="day" multiplier="120"/>
    </workflowDescriptor>
    <workflowDescriptor id="ApplyIncrementalEdits_Automatic" name="Apply Edits to Automatic Incremental Precipitation" forecast="false" visible="true" allowApprove="false">
        <description>Merge automatic incremental precipitation data (QC'ed, raw, edits)</description>
    </workflowDescriptor>
    <workflowDescriptor id="ExportPostGreSQL_AutomaticIncremental" name="Export QC'ed Automatic Incremental Precipitation Data to DB" forecast="false" visible="true" allowApprove="false">
        <description>Write the quality controlled incremental precipitation time series to the Hidrologia PostGreSQL database</description>
    </workflowDescriptor>
    <workflowDescriptor id="ExportPostGreSQL_StageReservoir" name="Export QC'ed Stage and Reservoir Data to DB" forecast="false" visible="true" allowApprove="false">
        <description>Write quality controlled stage and reservoir time series to the Hidrologia PostGreSQL database</description>
    </workflowDescriptor>

<workflowDescriptor id="ImportPostGreSQL_DailyGages" name="Import Daily Incremental Precipitation (do QC)" forecast="false" visible="true" allowApprove="false">
        <description>Import the scalar daily incremental precipitation time series from the Pronosticos PostGreSQL database</description>
        <runExpiryTime unit="day" multiplier="120"/>
    </workflowDescriptor>
    <workflowDescriptor id="ApplyIncrementalEdits_Daily" name="Apply Edits to Daily Incremental Precipitation" forecast="false" visible="true" allowApprove="false">
        <description>Merge daily incremental precipitation data (QC'ed, raw, edits)</description>
    </workflowDescriptor>
    <workflowDescriptor id="ExportPostGreSQL_DailyGages" name="Export QC'ed Daily Incremental Precipitation to DB" forecast="false" visible="true" allowApprove="false">
        <description>Write the quality controlled daily incremental precipitation time series to the Pronosticos PostGreSQL database</description>
    </workflowDescriptor>

<!-- Import workflows ==========================================================-->
    <workflowDescriptor id="ImportHistoricalPrecip" name="Import Historical gage precip" forecast="false" visible="false" allowApprove="false">
        <description>Import the historical precipitation time series from the Import directory and delete the files after importing</description>
    </workflowDescriptor>
    <workflowDescriptor id="MAP_HistoricalFromDailyOnly" name="Compute historical MAP from daily stations only" forecast="false" visible="false" allowApprove="false">
        <description>Compute MAP using only daily station data</description>
    </workflowDescriptor>
    <workflowDescriptor id="Export_MAPtoCSV" name="Export Historical MAP to CSV" forecast="false" visible="false" allowApprove="false">
        <description>Export MAP to CSV (prep for calibration)</description>
    </workflowDescriptor>
    <workflowDescriptor id="ImportHistoricalMAP" name="Import Historical MAPs (for ensemble forecasting)" forecast="false" visible="false" allowApprove="false">
        <description>Import the historical MAPs from the Import directory and delete the files after importing</description>
        <runExpiryTime unit="year" multiplier="100"/>
    </workflowDescriptor>
   <workflowDescriptor id="ImportWRF_ElSal" name="Import: WRF SV" forecast="false" autoApprove="true" visible="true">
      <cardinalTimeStep unit="hour" multiplier="6"/>
   </workflowDescriptor>
   <workflowDescriptor id="ImportGrids" name="Import Grids" forecast="false" visible="true" allowApprove="false" >
        <description>Import the gridded time series from the Import directory</description>
    </workflowDescriptor>

<!-- Preprocess workflows ======================================================-->
    <workflowDescriptor id="Preprocess" name="Preprocess stage to flow, MAP" forecast="true" visible="true" allowApprove="false">
        <description>Run all preprocessors</description>
        <runExpiryTime unit="day" multiplier="120"/>
    </workflowDescriptor>
    <workflowDescriptor id="Preprocess_flow" name="Process raw stage, convert to discharge" forecast="false" visible="false" allowApprove="false">
        <description>Convert 15-min stage to 1-hour stage, convert to discharge</description>
        <runExpiryTime unit="day" multiplier="120"/>
    </workflowDescriptor>
    <workflowDescriptor id="Preprocess_StationToGrids" name="Station to Gridded Precip" forecast="false" visible="false" allowApprove="false">
        <description>Convert station precip to gridded</description>
        <cardinalTimeStep id="13Z"/>
    </workflowDescriptor>
    <workflowDescriptor id="Preprocess_WRF" name="Convert WRF and CAWRF to incremental" forecast="true" visible="false" allowApprove="false">
        <description>Convert WRF and CAWRF to incremental</description>
    </workflowDescriptor>
    <workflowDescriptor id="Preprocess_MAP" name="Compute MAP from each source" forecast="true" visible="false" allowApprove="false">
        <description>Compute MAP from each source</description>
        <runExpiryTime unit="day" multiplier="120"/>
    </workflowDescriptor>

