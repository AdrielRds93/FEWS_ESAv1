<?xml version="1.0" encoding="UTF-8"?>
<thresholdWarningLevels xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns="http://www.wldelft.nl/fews"
    xsi:schemaLocation="http://www.wldelft.nl/fews http://schemas/thresholdWarningLevels.xsd"
>
        
	<thresholdWarningLevel id="default" name="No threshold exceeded">
		<color>blue1</color>
		<iconName>none.png</iconName>
		<historicOverlayIconName>none.png</historicOverlayIconName>
		<forecastOverlayIconName>none.png</forecastOverlayIconName>
	</thresholdWarningLevel>
	<thresholdWarningLevel id="WATCH" name="Watch">
		<color>yellow</color>
		<iconName>warninglevel2.gif</iconName>
		<historicOverlayIconName>historicwarninglevel2.gif</historicOverlayIconName>
		<forecastOverlayIconName>forecastwarninglevel2.gif</forecastOverlayIconName>
	</thresholdWarningLevel>
	<thresholdWarningLevel id="WARNING" name="Warning">
		<color>orange</color>
		<iconName>warninglevel4.gif</iconName>
		<historicOverlayIconName>historicwarninglevel3.gif</historicOverlayIconName>
		<forecastOverlayIconName>forecastwarninglevel3.gif</forecastOverlayIconName>
	</thresholdWarningLevel>
	<thresholdWarningLevel id="ALERT" name="Alert">
		<color>red</color>
		<iconName>warninglevel6.gif</iconName>
		<historicOverlayIconName>historicwarninglevel4.gif</historicOverlayIconName>
		<forecastOverlayIconName>forecastwarninglevel4.gif</forecastOverlayIconName>
	</thresholdWarningLevel>
	<thresholdWarningLevel id="LOWPOOL" name="Low Pool">
		<color>brown</color>
		<iconName>warninglevel2.gif</iconName>
		<historicOverlayIconName>historicwarninglevel2.gif</historicOverlayIconName>
		<forecastOverlayIconName>forecastwarninglevel2.gif</forecastOverlayIconName>
	</thresholdWarningLevel>

</thresholdWarningLevels>
