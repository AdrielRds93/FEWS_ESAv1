<locationSets version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/locationSets.xsd">
	<!--Estaciones-->
	<locationSet id="Stations">
		<csvFile>
			<file>Estaciones</file>
			<charset>UTF-8</charset>
			<id>%N_Oficial%</id>
			<name>%Nombre_estacion%</name>
			<x>%longitude%</x>
			<y>%latitude%</y>
			<attribute id="INSTITUCION">
				<text>%Institucion%</text>
			</attribute>
			<attribute id="PARAMETROS">
				<text>%Parametros_medidos%</text>
			</attribute>
			<attribute id="RESOLUCION">
				<text>%Resolucion_temporal%</text>
			</attribute>
			<attribute id="ESTACION">
				<text>%Tipo_estacion%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<locationSet id="ESA_P">
		<locationSetId>MARN_P</locationSetId>
	</locationSet>
	<locationSet id="ESA_H">
		<locationSetId>MARN_H</locationSetId>
	</locationSet>
	<locationSet id="ESA_Q">
		<locationSetId>MARN_Q</locationSetId>
	</locationSet>
	<locationSet id="ESA_HQ">
		<locationSetId>MARN_Q</locationSetId>
	</locationSet>
	<locationSet id="MARN">
		<locationSetId>Stations</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="INSTITUCION" contains="MARN"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="MARN_P">
		<locationSetId>MARN</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="PARAMETROS" contains="P"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="MARN_P_5min">
		<locationSetId>MARN_P</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="RESOLUCION" contains="5 min"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="MARN_P_10min">
		<locationSetId>MARN_P</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="RESOLUCION" contains="10 min"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="MARN_P_15min">
		<locationSetId>MARN_P</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="RESOLUCION" contains="5 min"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="MARN_P_diaria">
		<locationSetId>MARN_P</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="RESOLUCION" contains="Diaria"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="MARN_H">
		<locationSetId>MARN</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="PARAMETROS" contains="H"/>
			</anyValid>
		</constraints>
	</locationSet>
	<locationSet id="MARN_Q">
		<locationSetId>MARN</locationSetId>
		<constraints>
			<anyValid>
				<attributeTextContains id="PARAMETROS" contains="Q"/>
			</anyValid>
		</constraints>
	</locationSet>
	<!--Cuencas Artigas y Durazno-->
	<!--Estaciones Artigas y Durazno-->
	<!--Mapas de inundaciones-->
	<!-- Observaciones Series de Tiempo -->
	<!--Grids-->
	<locationSet id="Grids">
		<locationId>GFS</locationId>
		<locationId>GEFS</locationId>
	</locationSet>
	<!-- Locations for Calibration-->
</locationSets>
