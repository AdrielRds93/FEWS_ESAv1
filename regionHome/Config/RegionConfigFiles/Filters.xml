<filters version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/filters.xsd">
	<defaultFilterId>FEWS-ELSALVADOR</defaultFilterId>
	<!--FILTER For PI Web Service-->
	<filter id="FEWS-ELSALVADOR" name="Datos FEWS El Salvador">
		<child foreignKey="Observaciones"/>
	</filter>
	<!--FILTERS-->
	<filter id="Observaciones" name="Observaciones">
		<child foreignKey="Precipitación"/>
		<child foreignKey="Nivel"/>
		<child foreignKey="Caudal"/>
	</filter>
	<!--SUB-FILTERS-->
	<!--Observaciones-->
	<filter id="Precipitación" name="Precipitación">
		<child foreignKey="MARN_P"/>
	</filter>
	<filter id="Nivel" name="Nivel">
		<child foreignKey="MARN_H"/>
	</filter>
	<filter id="Caudal" name="Caudal">
		<child foreignKey="MARN_Q"/>
	</filter>
	<filter id="MARN_P" name="MARN">
		<validationIconsVisible>true</validationIconsVisible>
		<timeSeriesSet>
			<moduleInstanceId>PreprocessHQ</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.obs</parameterId>
			<locationSetId>MARN_P_5min</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="minute" multiplier="5"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>PreprocessHQ</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.obs</parameterId>
			<locationSetId>MARN_P_10min</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="minute" multiplier="10"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
		<timeSeriesSet>
			<moduleInstanceId>PreprocessHQ</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>P.obs</parameterId>
			<locationSetId>MARN_P_diaria</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep id="daily07"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
	</filter>
	<filter id="MARN_H" name="MARN">
		<validationIconsVisible>true</validationIconsVisible>
		<timeSeriesSet>
			<moduleInstanceId>PreprocessHQ</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>H.obs</parameterId>
			<locationSetId>MARN_H</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="minute" multiplier="15"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>1</synchLevel>
		</timeSeriesSet>
	</filter>
	<filter id="MARN_Q" name="MARN">
		<validationIconsVisible>true</validationIconsVisible>
		<timeSeriesSet>
			<moduleInstanceId>PreprocessHQ</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>Q.obs</parameterId>
			<locationSetId>MARN_Q</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="hour" multiplier="3"/>
			<relativeViewPeriod unit="day" start="-3" end="0"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</filter>
