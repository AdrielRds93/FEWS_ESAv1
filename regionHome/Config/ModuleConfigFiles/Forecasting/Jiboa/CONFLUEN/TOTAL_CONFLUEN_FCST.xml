<?xml version="1.0" encoding="UTF-8"?>
<transformationModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/transformationModule.xsd" version="1.0">

    <variable>
        <variableId>ILOPAN_ROUTED</variableId>
        <timeSeriesSet>
            <moduleInstanceId>LAGK_ILOPAN_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>ILOPAN</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>SANRAMON_ROUTED</variableId>
        <timeSeriesSet>
            <moduleInstanceId>LAGK_SANRAMON_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>SANRAMON</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>LOCAL</variableId>
        <timeSeriesSet>
            <moduleInstanceId>UNITHG_CONFLUEN_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>CONFLUEN</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>
    <variable>
        <variableId>TOTAL_SQIN</variableId>
        <timeSeriesSet>
            <moduleInstanceId>TOTAL_CONFLUEN_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>CONFLUEN</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>add originals</readWriteMode>
        </timeSeriesSet>
    </variable>

    <transformation id="ComputeTotal">
        <user>
            <simple>
                <expression>LOCAL + ILOPAN_ROUTED + SANRAMON_ROUTED</expression>
                <outputVariable>
                    <variableId>TOTAL_SQIN</variableId>
                </outputVariable>
            </simple>
        </user>
    </transformation>

</transformationModule>
