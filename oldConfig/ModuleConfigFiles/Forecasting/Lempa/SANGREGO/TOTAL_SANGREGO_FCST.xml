<?xml version="1.0" encoding="UTF-8"?>
<transformationModule xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/transformationModule.xsd" version="1.0">

    <variable>
        <variableId>NOVIE5_ROUTED</variableId>
        <timeSeriesSet>
            <moduleInstanceId>LAGK_5NOVIE_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>5NOVIE</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>COPINOLA_ROUTED</variableId>
        <timeSeriesSet>
            <moduleInstanceId>LAGK_COPINOLA_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>COPINOLA</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>MOCAL_ROUTED</variableId>
        <timeSeriesSet>
            <moduleInstanceId>LAGK_MOCAL_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>MOCAL</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>BAJOGUAR_ROUTED</variableId>
        <timeSeriesSet>
            <moduleInstanceId>LAGK_BAJOGUAR_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>BAJOGUAR</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>LOCAL</variableId>
        <timeSeriesSet>
            <moduleInstanceId>UNITHG_SANGREGO_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>SANGREGO</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
        </timeSeriesSet>
    </variable>
    <variable>
        <variableId>OBSERVED</variableId>
        <timeSeriesSet>
            <moduleInstanceId>StageQ</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>QIN</parameterId>
            <locationId>A_SANGREGO</locationId>
            <timeSeriesType>external historical</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>read only</readWriteMode>
            <ensembleId>main</ensembleId>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>TOTAL_SQIN</variableId>
        <timeSeriesSet>
            <moduleInstanceId>TOTAL_SANGREGO_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>SQIN</parameterId>
            <locationId>SANGREGO</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>add originals</readWriteMode>
        </timeSeriesSet>
    </variable>

    <variable>
        <variableId>TOTAL_QINE</variableId>
        <timeSeriesSet>
            <moduleInstanceId>TOTAL_SANGREGO_FCST</moduleInstanceId>
            <valueType>scalar</valueType>
            <parameterId>QINE</parameterId>
            <locationId>SANGREGO</locationId>
            <timeSeriesType>simulated forecasting</timeSeriesType>
            <timeStep unit="hour" multiplier="1"/>
            <relativeViewPeriod unit="hour" start="-240" startOverrulable="true" end="0" endOverrulable="true"/>
            <readWriteMode>add originals</readWriteMode>
        </timeSeriesSet>
    </variable>

    <transformation id="ComputeTotal">
        <user>
            <simple>
                <expression>LOCAL + NOVIE5_ROUTED + COPINOLA_ROUTED + MOCAL_ROUTED + BAJOGUAR_ROUTED</expression>
                <outputVariable>
                    <variableId>TOTAL_SQIN</variableId>
                </outputVariable>
            </simple>
        </user>
    </transformation>

    <transformation id="AdjustToObserved">
        <adjust>
            <adjustQUsingObservedInstantaneousDischarge>
                <observedDischarge>
                    <variableId>OBSERVED</variableId>
                </observedDischarge>
                <simulatedInstantaneousDischarge>
                    <variableId>TOTAL_SQIN</variableId>
                </simulatedInstantaneousDischarge>
                <moduleParameterFile>ADJUSTQ_SANGREGO_UPDT</moduleParameterFile>
                <adjustedForecastDischarge>
                    <variableId>TOTAL_QINE</variableId>
                </adjustedForecastDischarge>
            </adjustQUsingObservedInstantaneousDischarge>
        </adjust>
    </transformation>

</transformationModule>
