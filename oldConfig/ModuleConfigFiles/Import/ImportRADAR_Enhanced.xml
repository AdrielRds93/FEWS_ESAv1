<?xml version="1.0" encoding="UTF-8"?>
<timeSeriesImportRun xmlns="http://www.wldelft.nl/fews" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.wldelft.nl/fews http://schemas/timeSeriesImportRun.xsd">

	<import>
		<general>
			<importType>ArcInfoAsciiGrid</importType>
 			<folder>$IMPORT_FOLDER_ENHANCED_RADAR$</folder>
                        <failedFolder>$FAILED_FOLDER_ENHANCED_RADAR$</failedFolder>
                        <backupFolder>$BACKUP_FOLDER_ENHANCED_RADAR$</backupFolder>
			<idMapId>IdImportRADAR_Enhanced</idMapId>
			<importTimeZone>
				<timeZoneOffset>+00:00</timeZoneOffset>
			</importTimeZone>
			<geoDatum>WGS 1984</geoDatum>
		</general>
		<tolerance timeUnit="hour" unitCount="1" parameterId="QPE"/>
		<timeSeriesSet>
			<moduleInstanceId>ImportRADAR_Enhanced</moduleInstanceId>
			<valueType>grid</valueType>
			<parameterId>QPE</parameterId>
			<locationId>RADAR_Enhanced</locationId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="minute" multiplier="15"/>
			<readWriteMode>add originals</readWriteMode>
		</timeSeriesSet>
	</import>
	
</timeSeriesImportRun>
