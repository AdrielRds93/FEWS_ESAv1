<?xml version="1.0" encoding="UTF-8"?>
<thresholdGroups xmlns="http://www.wldelft.nl/fews" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.wldelft.nl/fews http://schemas/thresholds.xsd"
>
        <thresholdGroup id="Flooding">
                <defaultThreshold shortName="default"></defaultThreshold>
                <levelThreshold id="WATCH" name="Watch">
                        <upWarningLevelId>WATCH</upWarningLevelId>
                        <upIntId>1</upIntId>
                        <downIntId>2</downIntId>
                </levelThreshold>
                <levelThreshold id="WARNING" name="Warning">
                        <upWarningLevelId>WARNING</upWarningLevelId>
                        <upIntId>9</upIntId>
                        <downIntId>10</downIntId>
                </levelThreshold>
                <levelThreshold id="ALERT" name="Alert">
                        <upWarningLevelId>ALERT</upWarningLevelId>
                        <upIntId>1</upIntId>
                        <downIntId>2</downIntId>
                </levelThreshold>
        </thresholdGroup>	

    <thresholdGroup id="Pool Elevation">
                <defaultThreshold shortName="default"></defaultThreshold>
                <levelThreshold id="LOWPOOL" name="Low Pool">
                        <downWarningLevelId>LOWPOOL</downWarningLevelId>
                        <upIntId>100</upIntId>
                        <downIntId>101</downIntId>
                </levelThreshold>
        </thresholdGroup>

</thresholdGroups>
