<?xml version="1.0" encoding="UTF-8"?>
<idMap
    xmlns="http://www.wldelft.nl/fews"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.wldelft.nl/fews http://schemas/idMap.xsd"
    version="1.1"
>
    <!--Forecasted streamflow (adjusted to observed)-->
    <function internalLocationSet="Outlets" internalParameter="QINE" internalQualifier="Final" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="QR"/>
    <!--Forecasted streamflow (simulated)-->
    <function internalLocationSet="Outlets" internalParameter="SQIN" internalQualifier="Final" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="QR"/>
    <!--Mean areal precipitation-->
    <function internalLocationSet="AllCatchments" internalParameter="MAP" internalQualifier="Final" externalLocationFunction="@PostGreSQL_ID@" externalParameterFunction="PP"/>

    <enableOneToOneMapping/>
    
</idMap>
